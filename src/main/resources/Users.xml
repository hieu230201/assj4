<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entitys.Users">
        <table name="users" schema="ass" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="fullname">
                <column name="fullname"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="admin">
                <column name="admin" precision="4"/>
            </basic>
            <basic name="isActive">
                <column name="isActive" precision="4"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
