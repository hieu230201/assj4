<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entitys.Comments">
        <table name="comments" schema="ass" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="user_id">
                <column name="user_id" nullable="false" precision="11"/>
            </basic>
            <basic name="video_id">
                <column name="video_id" nullable="false" precision="11"/>
            </basic>
            <basic name="binhluan">
                <column name="binhluan" nullable="false"/>
            </basic>
            <basic name="dateComment">
                <column name="date_comment" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
