<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entitys.Share">
        <table name="share" schema="ass" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="userId">
                <column name="user_id" precision="11"/>
            </basic>
            <basic name="videoId">
                <column name="video_id" precision="11"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="shareDate">
                <column name="share_date" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
